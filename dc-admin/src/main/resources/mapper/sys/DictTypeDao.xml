<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="top.czttn.modules.sys.dao.DictTypeDao">

    <select id="ifExit" resultType="top.czttn.modules.sys.entity.DictTypeEntity">
        select *
        from sys_dict_type
        where name = #{dictType.name}
        limit 0, 1
    </select>


</mapper>